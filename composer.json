{
    "name": "idsolutions/laravel-module-generator",
    "description": "Generate Laravel Modules from a template.",
    "type": "laravel-module",
    "authors": [
        {
            "name": "ID Solutions",
            "email": "info@idsolutions.id"
        },
        {
            "name": "Muhammad Rheza Alfin",
            "email": "m.rheza.alfin@gmail.com"
        }
    ],
    "keywords": [
        "Laravel",
        "Laravel Modules",
        "Generator"
    ],
    "require": {
        "lorisleiva/laravel-actions": "^2.7.1",
        "symfony/yaml": "^7.0.0",
        "nwidart/laravel-modules": "^10.0.0|^11.0.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vheins\\LaravelModuleGenerator\\Providers\\LaravelModuleGeneratorServiceProvider"
            ],
            "aliases": {
                "ModuleGenerator": "Vheins\\LaravelModuleGenerator\\Facades\\LaravelModuleGenerator",
                "LaravelModuleGenerator": "Vheins\\LaravelModuleGenerator\\Facades\\LaravelModuleGenerator"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Vheins\\LaravelModuleGenerator\\": ""
        }
    }
}
